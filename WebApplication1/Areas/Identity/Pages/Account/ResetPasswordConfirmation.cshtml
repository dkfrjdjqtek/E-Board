@page
@model WebApplication1.Areas.Identity.Pages.Account.ResetPasswordConfirmationModel
@{
    ViewData["Title"] = S["RP_Title"];
}

<div class="alert alert-success">
    Your password has been reset successfully.
</div>
<p class="mt-3">
    <a asp-area="" asp-controller="Account" asp-action="Login" class="btn btn-primary">
        @S["Back_To_Login"]
    </a>
</p>
