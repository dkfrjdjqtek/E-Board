@model IEnumerable<WebApplication1.ViewModels.UserRowVM>
<h2>사용자 목록</h2>
<table class="table">
    <thead><tr><th>Email</th><th>이름</th><th>부서</th><th>직책</th><th></th></tr></thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.Email</td>
                <td>@x.DisplayName</td>
                <td>@x.Department</td>
                <td>@x.Position</td>
                <td><a asp-controller="Users" asp-action="Edit" asp-route-userId="@x.UserId">편집</a></td>
            </tr>
        }
    </tbody>
</table>
